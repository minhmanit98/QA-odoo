<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <template id="asset_frontend" inherit_id="web.assets_frontend">
      <xpath expr="." position="inside">
        <link rel="stylesheet" type="text/css" href="/qa_app/static/src/css/agency.min.css"  />
        <link rel="stylesheet" type="text/css" href="/qa_app/static/src/css/custom.css" />
        <link rel="stylesheet" type="text/scss" href="/qa_app/static/src/css/custom.scss"  />
        <script type="text/javascript" src="/qa_app/static/src/js/agency.min.js"/>
         <script type="text/javascript" src="/qa_app/static/src/js/navbar.js"/>
<!--        <link rel="stylesheet" type="text/scss" href="/qa_app/static/src/css/navbar.scss" rel="stylesheet" /> -->
      </xpath>
    </template>

    <template id="assets_frontend" inherit_id="web.assets_frontend" name="Website Assets">
      <xpath expr="." position="inside">
         <script type="text/javascript" src="/qa_app/static/src/js/menu.js"/>
           <script type="text/javascript" src="/qa_app/static/src/js/nav-min.js"/>
        <link rel="stylesheet" type="text/scss" href="/qa_app/static/src/css/nav-min.scss" />
<!--        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/css/bootstrap.min.css" />-->
      </xpath>
    </template>

      <template id="assets_frontend_inherit" inherit_id="web.assets_frontend" name="Scroll Button" >
        <xpath expr="." position="inside">
			<script type="text/javascript" src="/qa_app/static/src/js/scroll_totop.js"/>
        <a id="back-to-top" href="#" class="btn btn-light btn-lg back-to-top" role="button"><i class="fas fa-chevron-up"></i></a>
        </xpath>
    </template>
  </data>
</odoo>